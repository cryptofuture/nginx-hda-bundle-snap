ngx_addon_name=ngx_http_length_hiding_filter_module

if test -n "$ngx_module_link"; then
    ngx_module_type=HTTP_FILTER
    ngx_module_name=ngx_http_length_hiding_filter_module
    ngx_module_srcs="$ngx_addon_dir/ngx_http_length_hiding_filter_module.c"
    . auto/module
else
    HTTP_FILTER_MODULES="$HTTP_FILTER_MODULES ngx_http_length_hiding_filter_module"
    NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_http_length_hiding_filter_module.c"
fi
